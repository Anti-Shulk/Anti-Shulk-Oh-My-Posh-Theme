{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "var": {
    "windows_color": "#ff00ff"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#555555",
          "foreground_templates": [
            "{{ if matchP \"windows\" .OS}}#0077c2{{ end }}",
            "{{ if matchP \"ubuntu\" .OS}}#E95420{{ end }}"
          ],
          "background": "#fbfbfb",
          "leading_diamond": "\ue0b6",
          "template": "{{.Icon}} ",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue62a",
            "wsl": "\ue712"
          }
        },
        
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#e06c75",
          "template": " \ue799\u2800"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#555555",
          "background_templates": [
            "{{ if matchP \"windows\" .OS}}#0077c2{{ end }}",
            "{{ if matchP \"ubuntu\" .OS}}#E95420{{ end }}"
          ],
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "\uf07b",
            "folder_separator_icon": " <#000000>\ue0bd </>",
            "home_icon": "\ueb06",
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#000000",
          "background": "#00C853",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "template": " {{ .HEAD }}{{ if .Staging.Changed }}<#FF6F00> {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFD54F",
          "template": " "
        }
      ],
      "newline": true
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#ffffff",
          "template": " {{ if gt .Code 0 }}\uf52f{{ else }}\uf4a7{{ end }} ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "{{ .FormattedMs }} ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#555555",
    "foreground_templates": [
      "{{ if matchP \"windows\" .OS}}#0077c2{{ end }}",
      "{{ if matchP \"ubuntu\" .OS}}#E95420{{ end }}"
    ],
    "template": "‚ùØ "
  }
}
